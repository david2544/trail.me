    "html-loader": "1.3.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "ip": "1.1.5",
    "jest": "26.4.2",
    "jest-watch-typeahead": "0.6.0",
    "lint-staged": "10.2.13",
    "minimist": "1.2.5",
    "ngrok": "3.2.7",
    "node-sass": "4.14.1",
    "plop": "2.7.4",
